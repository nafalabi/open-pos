    "definitions": {
        "controller.LoginParams": {
            "type": "object",
            "properties": {
                "email": {
                    "type": "string",
                    "example": "admin@admin.com"
                },
                "password": {
                    "type": "string",
                    "example": "admin"
                }
            }
        },
        "enum.UserLevel": {
            "type": "integer",
            "enum": [
                0,
                1,
                2,
                3
            ],
            "x-enum-varnames": [
                "Admin",
                "Owner",
                "Manager",
                "Worker"
            ]
        },
        "model.CategoryFillable": {
            "type": "object",
            "required": [
                "name"
            ],
            "properties": {
                "name": {
                    "type": "string"
                }
            }
        },
        "model.ProductFillable": {
            "type": "object",
            "required": [
                "description",
                "name"
            ],
            "properties": {
                "categories": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "description": {
                    "type": "string"
                },
                "image": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "price": {
                    "type": "integer",
                    "minimum": 0
                },
                "stock": {
                    "description": "Image       string   `json:\"image\" validate:\"omitempty,uri\"` // ts types converter cant support uri for now",
                    "type": "integer",
                    "minimum": 0
                }
            }
        },
        "model.UserFillable": {
            "type": "object",
            "required": [
                "level",
                "name"
            ],
            "properties": {
                "email": {
                    "type": "string"
                },
                "level": {
                    "$ref": "#/definitions/enum.UserLevel"
                },
                "name": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "phone": {
                    "description": "`json:\"phone\" validate:\"e164\"`",
                    "type": "string"
                }
            }
        }
    },
